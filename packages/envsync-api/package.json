{
	"name": "envsync-api",
	"version": "0.4.0",
	"type": "module",
	"module": "index.ts",
	"scripts": {
		"build": "bun run builder.ts",
		"db": "bun run scripts/migrate.ts",
		"dev": "bun run --hot src/entrypoint.ts",
		"format": "prettier --write .",
		"start": "bun run src/entrypoint.ts",
		"test": "bun test tests/mock",
		"test:mock": "bun test tests/mock --timeout 10000",
		"test:e2e": "TEST_MODE=e2e bun test tests/e2e --preload tests/e2e/helpers/real-setup.ts --timeout 60000",
		"test:all": "bun test tests/ --timeout 30000",
		"e2e:init": "bun run ../../scripts/e2e-setup.ts init",
		"e2e:cleanup": "bun run ../../scripts/e2e-setup.ts cleanup",
		"sim": "bun run scripts/sim.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.821.0",
		"@aws-sdk/lib-storage": "^3.821.0",
		"@grpc/grpc-js": "^1.14.3",
		"@grpc/proto-loader": "^0.8.0",
		"@hono/zod-validator": "^0.7.0",
		"@openfga/sdk": "^0.8.0",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/api-logs": "^0.57.0",
		"@opentelemetry/core": "^1.30.0",
		"@opentelemetry/exporter-logs-otlp-http": "^0.57.0",
		"@opentelemetry/exporter-metrics-otlp-http": "^0.57.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
		"@opentelemetry/otlp-exporter-base": "^0.57.0",
		"@opentelemetry/otlp-transformer": "^0.57.0",
		"@opentelemetry/resources": "^1.30.0",
		"@opentelemetry/sdk-logs": "^0.57.0",
		"@opentelemetry/sdk-metrics": "^1.30.0",
		"@opentelemetry/sdk-node": "^0.57.0",
		"@opentelemetry/sdk-trace-base": "^1.30.0",
		"@opentelemetry/sdk-trace-node": "^1.30.0",
		"@opentelemetry/semantic-conventions": "^1.28.0",
		"@scalar/hono-api-reference": "^0.9.1",
		"@slack/webhook": "^7.0.5",
		"hono": "^4.7.11",
		"hono-openapi": "^0.4.8",
		"jose": "^6.1.3",
		"kysely": "^0.28.2",
		"mustache": "^4.2.0",
		"node-cache": "^5.1.2",
		"nodemailer": "^8.0.1",
		"openid-client": "^6.5.0",
		"openpgp": "^6.3.0",
		"pg": "^8.16.0",
		"pino": "^9.7.0",
		"redis": "^5.5.5",
		"sk-keygen": "^0.1.0",
		"uuid": "^11.1.0",
		"zod": "^3.25.50",
		"zod-openapi": "^4.2.4"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@bravo68web/prettier-config": "^1.2.6",
		"@types/bun": "latest",
		"@types/glob": "^8.1.0",
		"@types/mustache": "^4.2.6",
		"@types/nodemailer": "^7.0.9",
		"@types/pg": "^8.15.4",
		"esbuild": "^0.25.5",
		"eslint": "8",
		"glob": "^13.0.3",
		"prettier": "^3.5.3"
	},
	"prettier": "@bravo68web/prettier-config"
}