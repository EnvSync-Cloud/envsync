NODE_ENV=
PORT=
#// S3 configuration (RustFS default: rustfsadmin / rustfsadmin)
S3_BUCKET=
S3_REGION=
S3_ACCESS_KEY=rustfsadmin
S3_SECRET_KEY=rustfsadmin
S3_BUCKET_URL=
S3_ENDPOINT=
#// Redis configuration
CACHE_ENV=
REDIS_URL=
#// SMTP configuration
SMTP_HOST=
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=
SMTP_PASS=
SMTP_FROM=
#// Keycloak configuration (same as root .env; API reads from root)
