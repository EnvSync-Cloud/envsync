KEYCLOAK_CLI_CLIENT_ID=envsync-cli
KEYCLOAK_API_CLIENT_ID=envsync-api
KEYCLOAK_API_CLIENT_SECRET=
KEYCLOAK_WEB_REDIRECT_URI=
KEYCLOAK_WEB_CALLBACK_URL=
KEYCLOAK_API_REDIRECT_URI=
