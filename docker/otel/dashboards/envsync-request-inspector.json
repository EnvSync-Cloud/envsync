    {
      "title": "Trace Search Table",
      "description": "Search for traces matching the selected filters. Click a Trace ID to inspect the full waterfall and correlated logs below.",
      "type": "table",
      "id": 7,
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 28
      },
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "targets": [
        {
          "queryType": "traceql",
          "query": "{span.http.method =~ \".*\" && kind = server && resource.service.name = \"$service_name\" && span.http.route =~ \"${http_route:regex}\" && span.http.status_code =~ \"${status_code:regex}\" && duration > $min_duration} | select(span.http.method, span.http.route, span.http.status_code, span.envsync.user_id)",
          "tableType": "traces",
          "refId": "A",
          "limit": 50
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "traceID"
            },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "title": "Inspect Trace",
                    "url": "/d/envsync-request-inspector?var-traceId=${__data.fields.traceID}&${__url_time_range}",
                    "targetBlank": false
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ns"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Duration",
            "desc": true
          }
        ],
        "footer": {
          "enablePagination": true
        }
      },
      "transformations": []
    },
